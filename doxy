Banner
main(){
echo -e $cyan~+$red["1"] "$branco-$red Ver categorias"
echo -e $cyan~+$red["2"] "$branco-$red Updateo Repositordio"
echo -e $cyan~+$red["3"] "$branco-$red Atualizar soucelist"
echo -e $cyan~+$red["4"] "$branco-$red sobre"
echo -e $cyan~+$red["0"] "$branco-$red exit"$corPadrao
echo -e $cyan

  read -p "~:: " V

echo -e $corPadrao

   while [ "$V" != "1" -o "$V" != "2" -o "$V" != "3" -o "$V" != "4" -o "$V" != "0" ]; do

         if [ "$V" = "1" -o "$V" = "2" -o "$V" = "3" -o "$V" = "4" -o "$V" = "0" ]; then

    if [ "$V" = "0" ]; then
# saindo
echo -e "$vermelhoClaro Saindo...$corPadrao"
                    sleep 2

                    exit

        fi

    if [ "$V" = "2" ]; then

# Atualizando Repositorio
echo -e "$verdeClaro Atualizando Repositorio..."
                   sleep 1

                  apt-get update
echo -e "$corPadrao"
                 main

          fi

  if [ "$V" = "3" ]; then

echo -e "\e[01;32m Atualizando source list..."
             sleep 2
# Atualizando source list
echo "# For source package access, uncomment the following line" > /etc/apt/sources.list

echo "deb http://http.kali.org/kali kali-rolling main non-free contrib" >> /etc/apt/sources.list

echo "deb-src http://http.kali.org/kali kali-rolling main contrib non-free" >> /etc/apt/sources.list
echo "deb http://http.kali.org/kali kali-bleeding-edge contrib non-free main" >> /etc/apt/sources.list
echo "deb http://ftp.br.debian.org/debian/ jessie main non-free contrib" >> /etc/apt/sources.list
echo "deb-src http://ftp.br.debian.org/debian/ jessie main non-free contrib" >> /etc/apt/sources.list

echo "deb http://ftp.br.debian.org/debian/ jessie-updates main non-free contrib" >> /etc/apt/sources.list
echo "deb-src http://ftp.br.debian.org/debian/ jessie-updates main non-free contrib" >> /etc/apt/sources.list

echo "deb http://ftp.br.debian.org/debian/ jessie-backports main non-free contrib" >> /etc/apt/sources.list
echo "deb-src http://ftp.br.debian.org/debian/ jessie-backports main non-free contrib" >> /etc/apt/sources.list

echo "deb http://security.debian.org/ jessie/updates main contrib non-free" >> /etc/apt/sources.list
echo "deb-src http://security.debian.org/ jessie/updates main contrib non-free" >> /etc/apt/sources.list

echo "deb http://ftp.debian.org/debian jessie-proposed-updates main contrib non-free" >> /etc/apt/sources.list

              apt-get update
# Concluido
echo -e "\e[01;32m Concluido... sua source list foi Atualizada \e[0m"
               sleep 2

               main

    fi

    if [ "$V" = "4" ]; then

echo -e "$cyan
~+=[ --------- Sobre ------------ ]~+

- doxyTools e um script em shell, baseado no katoolin,
- de inicio contendo todas as Tools do katoolin,

- contendo muito mais agora, e continuarei adicionando muito mais Tool$corPadrao
"
                main
       fi

    if [ "$V" = "1" ]; then
# categorias
echo -e "\e[1;36m ~+=[ --------- Escolha uma categoria ------------ ]~+"
echo -e
        sleep 0
echo -e "
$cyan *  $branco[00)$cyan -$branco exit [0 )$cyan -$branco Home

$cyan *  $branco[01)$cyan -$branco Coleta de informacoes       $cyan*  $branco[09)$cyan -$branco Ferramentas Forenses
$cyan *  $branco[02)$cyan -$branco Analise de Vulnerabilidade  $cyan*  $branco[10)$cyan -$branco Teste de Estresse 
$cyan *  $branco[03)$cyan -$branco Ataque redes sem fio        $cyan*  $branco[11)$cyan -$branco Ataques de Senha  
$cyan *  $branco[04)$cyan -$branco Aplicacoes Web              $cyan*  $branco[12)$cyan -$branco Engenharia reversa
$cyan *  $branco[05)$cyan -$branco Sniffing & Spoofing         $cyan*  $branco[13)$cyan -$branco Hacking de Hardware
$cyan *  $branco[06)$cyan -$branco Mantendo o Acesso           $cyan*  $branco[14)$cyan -$branco Extra
$cyan *  $branco[07)$cyan -$branco Ferramentas de Relatorios   $cyan*  $branco[15)$cyan -$branco Dependencias
$cyan *  $branco[08)$cyan -$branco Ferramentas de Exploracao   $cyan*  $branco[16)$cyan -$branco Teste de penetracao
$corPadrao"

echo -e $cyan

  read -p "~:: " P

echo -e $corPadrao
       fi

    while [ "$P" != "01" -o "$P" != "02" -o "$P" != "03" -o "$P" != "04" -o "$P" != "05" -o "$P" != "06" -o "$P" != "07" -o "$P" != "08" -o "$P" != "09" -o "$P" != "10" -o "$P" != "11" -o "$P" != "12" -o "$P" != "13" -o "$P" != "14" -o "$P" != "16" -o "$P" != "15" -o "$P" != "00" -o "$P" != "0" ]; do

       if [ "$P" = "01" -o "$P" = "02" -o "$P" = "03" -o "$P" = "04" -o "$P" = "05" -o "$P" = "06" -o "$P" = "07" -o "$P" = "08" -o "$P" = "09" -o "$P" = "10" -o "$P" = "11" -o "$P" = "12" -o "$P" = "13" -o "$P" = "14" -o "$P" = "15" -o "$P" = "16" -o "$P" = "00" -o "$P" = "0" ]; then

         if [ "$P" = "0" ]; then

echo -e "\e[1;36m~Home~\e[0m"
          main

           fi

         while [ "$P" = "16" ]; do

echo -e "\e[1;36m Penetration TestingÂ \e[1;m"
   sleep 1

echo -e "$cyan

*   [00) - exit  [0 ) - Home

*   [01) PytheM



  $corPadrao"

echo -e $cyan

  read -p "~:: " Tg

echo -e $corPadrao

   while [ "$Tg" != "01" -o "$Tg" != "0" -o "$Tg" != "00" ]; do

      if [ "$Tg" = "01" -o "$Tg" = "0" -o "$Tg" = "00" ]; then

         if [ "$Tg" = "0" ]; then

echo -e "\e[1;36m~Home~\e[0m"
          main

           fi

          if [ "$Tg" = "00" ]; then

# saindo
echo -e "$vermelhoClaro Saindo...$corPadrao"
                    sleep 2

                    exit

                fi

         if [ "$Tg" = "01" ]; then

echo -e "\e[0;36m instalando pytheM...\e[0m"
            sleep 2

            apt-get update 
            apt-get install git python-pip build-essential python-dev tcpdump libnetfilter-queue-dev libffi-dev libssl-dev -y && git clone https://github.com/m4n3dw0lf/PytheM
            cd PytheM
            pip install -r requirements.txt
            chmod a+x ./pythem.py
            ./pythem.py

echo -e "\e[1;32m Pronto * \e[0m"
echo -e $cyan

  read -p "~:: " Dp

echo -e $corPadrao
               fi

           else

echo -e $cyan

  read -p "~:: " Tg

echo -e $corPadrao
         fi

      done

   done

          if [ "$P" = "00" ]; then

# saindo
echo -e "$vermelhoClaro Saindo...$corPadrao"
                    sleep 2

                    exit

                fi

          while [ "$P" = "15" ]; do

# Dependencies
echo -e "\e[1;36m Dependencies\e[1;m"
   sleep 1

echo -e "$cyan

*   [00) - exit  [0 ) - Home

*   [01) python-dnspython
*   [02) nano              
