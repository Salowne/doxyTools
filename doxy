echo -e "\e[1;32m Pronto * \e[0m"
echo -e $cyan

  read -p "~:: " IG

echo -e $corPadrao
         fi

               else

echo -e $cyan

  read -p "~:: " IG

echo -e $corPadrao
                 fi

                done

               done

              else

echo -e $cyan

  read -p "~:: " P

echo -e $corPadrao
               fi

            done

          else

echo -e $cyan

  read -p "~:: " V

