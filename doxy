*   [26) python-geoip
*   [27) python-pycurl
*   [28) python-nmap
*   [29) python-whois
*   [30) toilet
*   [31) figlet
  $corPadrao"

echo -e $cyan

  read -p "~:: " Dp

echo -e $corPadrao

    while [ "$Dp" != "01" -o "$Dp" != "02" -o "$Dp" != "03" -o "$Dp" != "04" -o "$Dp" != "05" -o "$Dp" != "06" -o "$Dp" != "07" -o "$Dp" != "08" -o "$Dp" != "09" -o "$Dp" != "10" -o "$Dp" != "11" -o "$Dp" != "12" -o "$Dp" != "13" -o "$Dp" != "14" -o "$Dp" != "15" -o "$Dp" != "16" -o "$Dp" != "17" -o "$Dp" != "18" -o "$Dp" != "19" -o "$Dp" != "20" -o "$Dp" != "21" -o "$Dp" != "22" -o "$Dp" != "23" -o "$Dp" != "00" -o "$Dp" != "0" -o "$Dp" != "24" -o "$Dp" != "25" -o "$Dp" != "26" -o "$Dp" != "27" -o "$Dp" != "28" -o "$Dp" != "29" -o "$Dp" != "30" -o "$Dp" != "31" ]; do

       if [ "$Dp" = "01" -o "$Dp" = "02" -o "$Dp" = "03" -o "$Dp" = "04" -o "$Dp" = "05" -o "$Dp" = "06" -o "$Dp" = "07" -o "$Dp" = "08" -o "$Dp" = "09" -o "$Dp" = "10" -o "$Dp" = "11" -o "$Dp" = "12" -o "$Dp" = "13" -o "$Dp" = "14" -o "$Dp" = "15" -o "$Dp" = "16" -o "$Dp" = "17" -o "$Dp" = "18" -o "$Dp" = "19" -o "$Dp" = "20" -o "$Dp" = "21" -o "$Dp" = "22" -o "$Dp" = "23" -o "$Dp" = "00" -o "$Dp" = "0" -o "$Dp" = "24" -o "$Dp" = "25" -o "$Dp" = "26" -o "$Dp" = "27" -o "$Dp" = "28" -o "$Dp" = "29" -o "$Dp" = "30" -o "$Dp" = "31" ]; then

